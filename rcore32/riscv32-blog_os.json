{
        "llvm-target": "riscv32",
        "data-layout": "e-m:e-p:32:32-i64:64-n32-S128",
        "arch": "riscv32",
        "cpu": "generic-rv32",
        "target-endian": "little",
        "target-pointer-width": "32",
        "target-c-int-width": "32",
        "os": "none",
        "executables": true,
        "linker-flavor": "ld.lld",
        "linker": "rust-lld",
        "panic-strategy": "abort",
        "disable-redzone": true,
        "features": "",
        "relocation-model": "static",
        "abi-blacklist": [
                "cdecl",
                "stdcall",
                "fastcall",
                "vectorcall",
                "thiscall",
                "aapcs",
                "win64",
                "sysv64",
                "ptx-kernel",
                "msp430-interrupt"
          ]
}
